<h1>Create New Project Page!</h1>

<%= form_for [@project] do |f| %>
  <div class='form-group'>
    <%= f.label :title %>
    <%= f.text_field :title, class: 'form-control' %>
  </div>
  <div class='form-group'>
  <%= f.label :description %>
    <%= f.text_field :description, class: 'form-control' %>
    </div>
  <div class='form-group'>
    <%= f.label :complete %>
    <%= f.select(:complete, options_for_select([['Complete', true], ['In Progress', false ]]), class: 'form-control' ) %>
  </div>
  <%= f.submit 'Create New Project', class: 'btn btn-success' %>
<% end %>